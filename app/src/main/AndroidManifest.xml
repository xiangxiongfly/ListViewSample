<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication">
        <activity android:name=".expandable.ExpandableListViewActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".arrayadapter.ArrayAdapterActivity"
            android:label="ArrayAdapter" />
        <activity
            android:name=".simpleadapter.SimpleAdapterActivity"
            android:label="SimplAdapter" />
        <activity
            android:name=".baseadapter.BaseAdapterActivity"
            android:label="BaseAdapter" />
        <activity
            android:name=".category.CategoryListViewActivity"
            android:label="分类型ListView" />
        <activity
            android:name=".checkbox.CheckBoxListViewActivity"
            android:label="ListView与CheckBox" />
    </application>

</manifest>